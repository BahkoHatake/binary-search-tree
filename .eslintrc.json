{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":[ "airbnb-base",
    "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["main.js"],
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-param-reassign":"off",
        "consistent-return":"off"
    }
}
